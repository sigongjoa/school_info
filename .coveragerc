[run]
branch = True
source = src,.
omit =
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */.venv/*
    */downloads/*
    */output/*
    */chroma_db/*
    */chroma_test/*
    */debug_artifacts/*
    */school_docs/*
    */templates/*
    */docs/*
    */enhanced_jsons/*
    generate_*.py
    verify_*.py
    quick_test.py

[report]
precision = 2
show_missing = True
skip_covered = False
fail_under = 100.00

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
